&usb {
	status = "okay";
	dr_mode = "otg";
	usb-role-switch;
	ss-select-gpios = <&gpio GPIOA_13 GPIO_ACTIVE_LOW>;

	vbus-supply = <&reg_usb_otg>;

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@1 {
			reg = <1>;
			usb3_role_switch: endpoint {
				remote-endpoint = <&hd3ss3220_out_ep>;
			};
		};
	};
};

&dwc3 {
	vbus-supply = <&reg_usb_host>;
};

&usb2_phy0 {
	phy-supply = <&reg_5v0>;
};

&usb2_phy1 {
	phy-supply = <&reg_5v0>;
};

&usb3_pcie_phy {
