	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		power-button {
			label = "power";
			linux,code = <KEY_POWER>;
			gpios = <&gpio_ao GPIOAO_7 GPIO_ACTIVE_LOW>;
		};
	};

	gpiomem {
		compatible = "amlogic, gpiomem";
		reg = <0x0 0xff634000 0x0 0x1000>;
		dev_name = "gpiomem";
		status = "okay";
	};

	gpiomem_ao {
		compatible = "amlogic, gpiomem";
		reg = <0x0 0xff800000 0x0 0x1000>;
		dev_name = "gpiomem-ao";
		status = "okay";
	};

#if 0
	leds {
		compatible = "gpio-leds";

		led-white {
			color = <LED_COLOR_ID_WHITE>;
			function = LED_FUNCTION_STATUS;
			gpios = <&gpio_ao GPIOAO_4 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "heartbeat";
		};
	};
#endif

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x40000000>;
	};

	reserved-memory {
		galcore_reserved:linux,galcore {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x0 0x1000000>;
			alignment = <0x0 0x400000>;
			linux,contiguous-region;
		};
	};

	reg_1v8: regulator-reg_1v8 {
		compatible = "regulator-fixed";
		regulator-name = "VCC_1V8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	reg_3v3: regulator-reg_3v3 {
		compatible = "regulator-fixed";
		regulator-name = "VSYS_3V3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	reg_5v0: regulator-reg_5v0 {
		compatible = "regulator-fixed";
		regulator-name = "VCC_5V";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		enable-active-high;
	};

	reg_usb_otg: regulator-reg-usb-otg {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio GPIOH_8 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		regulator-max-microvolt = <5000000>;
		regulator-min-microvolt = <5000000>;
		regulator-name = "USB_OTGPWR";
	};

	reg_usb_host: regulator-reg-usb-host {
		compatible = "regulator-fixed";
		regulator-name = "USB_HOSTPWR";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio GPIOA_10 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <&gpio GPIOX_6 GPIO_ACTIVE_LOW>;
		clocks = <&wifi32k>;
		clock-names = "ext_clock";
	};

	hdmi-connector {
		compatible = "hdmi-connector";
		type = "a";

		port {
			hdmi_connector_in: endpoint {
				remote-endpoint = <&hdmi_tx_tmds_out>;
			};
		};
	};

	sound {
		compatible = "amlogic,axg-sound-card";
		model = "boundary-a311d";
		audio-aux-devs = <&tdmout_a>, <&tdmin_b>, <&tdmout_b>;
		audio-widgets = "Speaker", "Ext Spk",
				"Headphone", "Headphone Jack",
				"Microphone", "AMIC",
				"Microphone", "AMIC2";
		audio-routing =
				"Ext Spk", "WM SPK_LN",
				"Ext Spk", "WM SPK_LP",
				"Ext Spk", "WM SPK_RN",
				"Ext Spk", "WM SPK_RP",
				"Headphone Jack", "WM HP_L",
				"Headphone Jack", "WM HP_R",
				"WM LINPUT1", "AMIC",
				"AMIC", "WM MICB",
				"WM RINPUT1", "AMIC2",
				"AMIC2", "WM MICB",
				"TDM_B Capture", "WM Capture",
				"WM Playback", "TDM_B Playback",

				/*
				 * The below routes are unchanged for all boards.
				 * But may be commented out to force a path.
				 * ie. FRDDR_A to wolfson codec only
				 * FRDDR_B to hdmi only
				 */
				/* 0-2:frddr_a-c*,  0-2:tdmout_a-c | 3-4:spdifout-b */
				"TDMOUT_B IN 0", "FRDDR_A OUT 1",	/* Wolfson path */
				"TDMOUT_A IN 1", "FRDDR_B OUT 0",	/* hdmi path */
#if 0
				"TDMOUT_A IN 0", "FRDDR_A OUT 0",
				"TDMOUT_B IN 1", "FRDDR_B OUT 1",
				"TDMOUT_A IN 2", "FRDDR_C OUT 0",
				"TDMOUT_B IN 2", "FRDDR_C OUT 1",
#endif
				/* 0-2:tdmin_a-c | 3:spdifin | 4:pdmin | 6:tdmin_lb,  */
				"TODDR_A IN 1", "TDMIN_B OUT",
#if 0
				"TODDR_B IN 1", "TDMIN_B OUT",
				"TODDR_C IN 1", "TDMIN_B OUT",
#endif
#define PLAY_HDMI_WOLFSON_TOGETHER	/* TOHDMITX I2S SRC - I2S A(separate), I2S B(together) */
#ifndef PLAY_HDMI_WOLFSON_TOGETHER
				"TDM_A Playback", "TDMOUT_A OUT",
#endif
				"TDM_B Playback", "TDMOUT_B OUT",
				"TDMIN_B IN 0", "TDM_B Capture";
		assigned-clocks = <&clkc CLKID_MPLL2>,
				  <&clkc CLKID_MPLL0>,
				  <&clkc CLKID_MPLL1>;
		assigned-clock-parents = <0>, <0>, <0>;
		assigned-clock-rates = <294912000>,
				       <270950400>,
				       <393216000>;
		status = "okay";

		dai-link-0 {
			sound-dai = <&frddr_a>;
		};

		dai-link-1 {
			sound-dai = <&frddr_b>;
		};

#if 0
		dai-link-2 {
			sound-dai = <&frddr_c>;
		};
#endif
		dai-link-3 {
			sound-dai = <&toddr_a>;
		};

#if 0
		dai-link-4 {
			sound-dai = <&toddr_b>;
		};

		dai-link-5 {
			sound-dai = <&toddr_c>;
		};
#endif
#ifdef PLAY_HDMI_WOLFSON_TOGETHER
		/* i2s wm8960 interface */
		/* 8ch hdmi interface */
		dai-link-6 {
			dai-format = "i2s";
			dai-tdm-slot-rx-mask-0 = <1 1>;
			dai-tdm-slot-tx-mask-0 = <1 1>;
			dai-tdm-slot-tx-mask-1 = <1 1>;
			dai-tdm-slot-tx-mask-2 = <1 1>;
			dai-tdm-slot-tx-mask-3 = <1 1>;
			mclk-fs = <256>;
			sound-dai = <&tdmif_b>;

			codec-0 {
				sound-dai = <&wm8960 0>;
			};

			codec-1 {
				sound-dai = <&tohdmitx TOHDMITX_I2S_IN_B>;
			};
		};
#else
		/* i2s wm8960 interface */
		dai-link-6 {
			dai-format = "i2s";
			dai-tdm-slot-rx-mask-0 = <1 1>;
			dai-tdm-slot-tx-mask-0 = <1 1>;
			mclk-fs = <256>;
			sound-dai = <&tdmif_b>;

			codec {
				sound-dai = <&wm8960 0>;
			};
		};

		/* 8ch hdmi interface */
		dai-link-7 {
			dai-format = "i2s";
			dai-tdm-slot-tx-mask-0 = <1 1>;
			dai-tdm-slot-tx-mask-1 = <1 1>;
			dai-tdm-slot-tx-mask-2 = <1 1>;
			dai-tdm-slot-tx-mask-3 = <1 1>;
			mclk-fs = <256>;
			sound-dai = <&tdmif_a>;

			codec {
				sound-dai = <&tohdmitx TOHDMITX_I2S_IN_A>;
			};
		};
#endif

		/* hdmi glue */
		dai-link-8 {
			sound-dai = <&tohdmitx TOHDMITX_I2S_OUT>;

			codec {
				sound-dai = <&hdmi_tx>;
			};
		};
	};

	wifi32k: wifi32k {
		compatible = "pwm-clock";
		#clock-cells = <0>;
		clock-frequency = <32768>;
		pwms = <&pwm_ef 0 30518 0>; /* PWM_E at 32.768KHz */
	};
};

&arb {
	status = "okay";
};

&clkc_audio {
	status = "okay";
};

&cec_AO {
	pinctrl-0 = <&cec_ao_a_h_pins>;
	pinctrl-names = "default";
	status = "disabled";
	hdmi-phandle = <&hdmi_tx>;
};

&cecb_AO {
	pinctrl-0 = <&cec_ao_b_h_pins>;
	pinctrl-names = "default";
	status = "okay";
	hdmi-phandle = <&hdmi_tx>;
};

