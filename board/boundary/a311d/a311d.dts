/dts-v1/;

#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/gpio/meson-g12a-gpio.h>
#include <dt-bindings/sound/meson-g12a-tohdmitx.h>
#include "meson-g12b-a311d.dtsi"

/ {
	compatible = "boundary,a311d", "amlogic,a311d", "amlogic,g12b";
	model = "Boundary Devices A311D";

	aliases {
		ethernet0 = &ethmac;
		i2c0 = &i2c_AO;
		i2c1 = &i2c2;
		i2c2 = &i2c3;
		rtc1 = &vrtc;
		serial0 = &uart_AO;
		serial1 = &uart_AO_B;
	};

	backlight_mipi: backlight-mipi {
		compatible = "gpio-backlight";
		gpios = <&gpio GPIOA_11 GPIO_ACTIVE_HIGH>;
		default-on;
		status = "okay";
	};

	bt-rfkill {
		compatible = "net,rfkill-gpio";
		name = "bt-rfkill";
		type = <2>; /* bluetooth */
		reset-gpios = <&gpio GPIOX_19 GPIO_ACTIVE_LOW>;
		status = "okay";
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	connector {
		compatible = "usb-c-connector";
		label = "USB-C";
		data-role = "dual";

		#address-cells = <1>;
		#size-cells = <0>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@1 {
				reg = <1>;
				con_ep: endpoint {
					remote-endpoint = <&hd3ss3220_in_ep>;
				};
			};
		};
	};

	emmc_pwrseq: emmc-pwrseq {
		compatible = "mmc-pwrseq-emmc";
		reset-gpios = <&gpio BOOT_12 GPIO_ACTIVE_LOW>;
	};

#if 0
	galcore {
